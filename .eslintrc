{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Class": true,
    "merge": true,
    "bind": true,
    "CACHE": true,
    "describe": true,
    "require": true,
    "it": true,
    "global": true,
    "expect": true,
    "before": true,
    "beforeEach": true,
    "afterEach": true,
    "after": true
  },
  "rules": {
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 2,
    "no-use-before-define": 2,
    "complexity": [
      1,
      15
    ],
    "max-depth": [
      1,
      3
    ],
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "no-extra-parens": 0,
    "strict": [
      2,
      "function"
    ],
    "no-undef": 2,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "after-used"
      }
    ],
    "no-empty": 2,
    "func-style": [
      2,
      "expression"
    ],
    "brace-style": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-nested-ternary": 2,
    "operator-linebreak": [
      2,
      "after"
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed",
      {
        "keywords": true
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-spaced-func": 2,
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "yoda": [
      2,
      "never"
    ],
    "max-len": [
      2,
      100
    ],
    "new-cap": 2,
    "comma-style": [
      2,
      "last"
    ],
    "dot-notation": 2,
    "newline-after-var": [
      2,
      "always"
    ],
    "eol-last": 2,
    "one-var": [
      2,
      "always"
    ],
    "lines-around-comment": [
      1,
      {
        "beforeLineComment": true
      }
    ],
    "wrap-iife": 2,
    "semi": [
      2,
      "always"
    ],
    "space-infix-ops": 2,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "else": {
            "before": true
          },
          "while": {
            "before": true
          },
          "catch": {
            "before": true
          }
        }
      }
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "always"
    ],
    "vars-on-top": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "double"
    ]
  }
}
